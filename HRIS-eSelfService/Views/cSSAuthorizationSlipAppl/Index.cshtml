    <!-- Add Modal -->
    <div class="modal fade" id="main_modal" role="dialog" aria-labelledby="modalLabelSmall" aria-hidden="true">
        <div class="modal-dialog modal-lg">
            <div class="modal-content animated flipInY">
                <div class="modal-header modal-header-add-edit text-white">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <h4 class="modal-title" id="modalLabelSmall">{{ModalTitle}}</h4>
                </div>
                <div class="modal-body with-background" style="padding-bottom:5px;padding-top:5px;">
                    <div class="row">
                        <div class="col-sm-12">
                            <b>AUTHORIZATION SLIP INFORMATION</b>
                        </div>
                    </div>
                    <hr style="margin-top:5px;margin-bottom:2px;border:0.5px solid cadetblue;padding:0px;" />
                    <div class="row" style="margin-top:5px;">
                        <div class="col-lg-2">
                            <b><label class="pull-left" style="margin-top:0px;padding-top:5px;">Application Nbr.:</label></b>
                        </div>
                        <div class="col-lg-6">
                            <b><input class="form-control text-center" id="txtb_appl_nbr" ng-model="txtb_appl_nbr" disabled /></b>
                            <label id="lbl_txtb_sig1_name_req" class="lbl_required"></label>
                        </div>
                        <div class="col-lg-2">
                            <b><label class="pull-left" style="margin-top:0px;padding-top:5px;">Date Applied:</label></b>
                        </div>
                        <div class="col-lg-2">
                            <b><input class="form-control text-center" id="txtb_date_applied" ng-model="txtb_date_applied" disabled/></b>
                            <label id="lbl_txtb_sig1_name_req" class="lbl_required"></label>
                        </div>
                    </div>
                    <div class="row" style="padding-top:5px;">
                        <div class="col-lg-2">
                            <b><label class="pull-left" style="margin-top:0px;padding-top:5px;">Employee's Name:</label></b>
                        </div>
                        <div class="col-lg-6">
                            <b><input class="form-control text-left" id="txtb_empl_name" ng-model="txtb_empl_name" disabled /></b>
                            <label id="lbl_txtb_sig1_name_req" class="lbl_required"></label>
                        </div>
                        <div class="col-lg-2">
                            <b><label class="pull-left" style="margin-top:0px;padding-top:5px;">ID Nbr.:</label></b>
                        </div>
                        <div class="col-lg-2">
                            <b><input class="form-control text-center" id="txtb_empl_id" ng-model="txtb_empl_id" disabled /></b>
                            <label id="lbl_txtb_empl_id_req" class="lbl_required"></label>
                        </div>
                    </div>
                    

                    <div class="row" style="padding-top:5px;" ng-show="show_creator">
                        <div class="col-sm-8">
                            <div style="width:27%; float:left;">
                                <b><label class="pull-left" style="margin-top:0px;padding-top:5px;">Created By:</label></b>
                            </div>
                            <div style="width:73%; float:left;">
                                <b><input class="form-control text-left" id="txtb_created_by" ng-model="txtb_created_by" disabled /></b>
                                <label id="lbl_txtb_creator_name_req" class="lbl_required"></label>
                            </div>
                        </div>
                    </div>
                    <div class="row" style="padding-top:5px;">
                        <div class="col-lg-2">
                            <b><label class="pull-left" style="margin-top:0px;padding-top:5px;">Remarks:</label></b>
                        </div>
                        <div class="col-lg-6">
                            <textarea class="form-control text_area" maxlength="255" rows="1" ng-model="txtb_remarks" id="txtb_remarks" disabled></textarea>
                            <label id="lbl_txtb_remarks_req" class="lbl_required"></label>
                        </div>
                        <div class="col-lg-4"></div>
                    </div>


                    <hr style="margin-top:5px;margin-bottom:2px;border:0.5px solid cadetblue;padding:0px;" />
                    <div class="row" style="padding-top:5px;">
                        <div class="col-lg-2">
                            <label style="padding-top:7px;"><b>DTR DATE ENTRY:</b></label>
                        </div>
                        <div class="col-lg-4 mt10">
                            <div class="form-group" id="div_dtr_date" style="margin-bottom:0px;">
                                <div class="input-group date">
                                    <input type="text" id="txtb_dtr_date" maxlength="10" ng-model="txtb_dtr_date" class="form-control text-center" ng-disabled="dis_plan_date">
                                    <span class="input-group-addon dtr_date" ng-show="show_calendar_icon" style="padding-left:7.5px;padding-bottom:0px !important;">
                                        <i class="fa fa-calendar dtr_date"></i>
                                    </span>
                                </div>
                            </div>
                            @*<label id="lbl_txtb_dtr_date_req" class="lbl_required"></label>*@
                        </div>

                        <div class="col-lg-2 mt10">
                            <label class="pull-left font-bold" style="padding-top:7px;font-size:12px;"><b>A.S TYPE:</b></label>
                        </div>
                        <div class="col-lg-3">
                            <select class="form-control field-select-sm" id="ddl_as_type" ng-disabled="dis_as_type" ng-model="ddl_as_type" ng-change="ddl_as_type_change()">
                                <option value="" selected>--Select Here--</option>
                                <option ng-repeat="lst in as_type_list" value="{{lst.astype_code}}">{{lst.astype_descr}}</option>
                            </select>
                            @*<label id="lbl_ddl_as_type_req" class="lbl_required"></label>*@
                        </div>
                        <div class="col-lg-1">
                            <button id="add2" ng-show="show_calendar_icon" class="btn btn-success font-bold" ng-click="btn_add_date()"><i class="fa fa-plus-circle"> </i> ADD</button>
                        </div>
                    </div>
                    <div class="row" style="margin-top:3px;">
                        <div class="col-lg-2">
                            <label id="lbl_am_entry">AM TIME ENTRY:</label>
                        </div>
                        <div class="col-lg-2" style="margin-top:3px;">
                            <div class="input-group clockpicker" data-autoclose="true">
                                <input type="text" ng-disabled="dtl_enable" class="form-control text-success ng-pristine ng-untouched ng-valid ng-empty ng-valid-maxlength" maxlength="7" placeholder="TIME IN" id="txtb_am_in" ng-model="txtb_am_in">
                                <span class="input-group-addon am_in">
                                    <span class="fa fa-clock-o"></span>
                                </span>
                            </div>
                            @*<label id="lbl_txtb_am_in_req" class="lbl_required"></label>*@
                        </div>



                        <div class="col-lg-2" style="margin-top:3px;">
                            <div class="input-group clockpicker" data-autoclose="true">
                                <input type="text" ng-disabled="is_to" class="form-control text-danger ng-pristine ng-untouched ng-valid ng-empty ng-valid-maxlength" maxlength="7" placeholder="TIME OUT" id="txtb_am_out" ng-model="txtb_am_out">
                                <span class="input-group-addon am_out">
                                    <span class="fa fa-clock-o"></span>
                                </span>
                            </div>
                            @*<label id="lbl_txtb_am_out_req" class="lbl_required"></label>*@
                        </div>


                        <div class="col-lg-2">
                            <label id="lbl_pm_entry"></label>
                        </div>

                        <div class="col-lg-2" style="margin-top:3px;">
                            <div class="input-group clockpicker" data-autoclose="true">
                                <input type="text" ng-disabled="is_to" class="form-control text-success ng-pristine ng-untouched ng-valid ng-empty ng-valid-maxlength" maxlength="7" placeholder="TIME IN" id="txtb_pm_in" ng-model="txtb_pm_in">
                                <span class="input-group-addon pm_in">
                                    <span class="fa fa-clock-o"></span>
                                </span>
                            </div>
                            @*<label id="lbl_txtb_pm_in_req" class="lbl_required"></label>*@
                        </div>

                        <div class="col-lg-2" style="margin-top:3px;">

                            <div class="input-group clockpicker" data-autoclose="true">
                                <input type="text" ng-disabled="dtl_enable" class="form-control text-danger ng-pristine ng-untouched ng-valid ng-empty ng-valid-maxlength" maxlength="7" placeholder="TIME OUT" id="txtb_pm_out" ng-model="txtb_pm_out">
                                <span class="input-group-addon pm_out">
                                    <span class="fa fa-clock-o"></span>
                                </span>
                            </div>
                            @*<label id="lbl_txtb_pm_out_req" class="lbl_required"></label>*@
                        </div>
                    </div>

                    <div class="row mt10">
                        <div class="col-sm-2">
                            <label class="font-bold"><b>AS Reason:</b></label>
                        </div>
                        <div class="col-sm-4">
                            <select ng-disabled="dtl_enable" class="form-control field-select-sm" id="ddl_reason" ng-model="ddl_reason">
                                <option value="" selected>--Select Here--</option>
                                <option ng-repeat="lst in reason_list" value="{{lst.reason_code}}">{{lst.reason_descr}}</option>
                            </select>
                            @*<label id="lbl_ddl_reason_req" class="lbl_required"></label>*@
                        </div>
                        <div class="col-sm-2">
                            <label class="font-bold"><b>AS Venue:</b></label>
                        </div>
                        <div class="col-sm-4">
                            <b><input ng-disabled="dtl_enable" class="form-control text-left" id="txtb_as_venue" maxlength="255" ng-model="txtb_as_venue" /></b>
                            @*<label id="lbl_txtb_as_venue_req" class="lbl_required"></label>*@
                        </div>
                    </div>
                    <div class="row mt10">
                        <div class="col-lg-2">
                            <b><label class="pull-left" style="margin-top:0px;padding-top:5px;">AS Comment:</label></b>
                        </div>
                        <div class="col-lg-4">
                            <textarea ng-disabled="dtl_enable" class="form-control text_area" rows="1" maxlength="255" id="txtb_as_comment" ng-model="txtb_as_comment"></textarea>
                            <label id="lbl_txtb_as_comment_req" class="lbl_required"></label>
                        </div>
                    </div>

                    <hr style="margin-top:5px;margin-bottom:2px;border:0.5px solid cadetblue;padding:0px;" />
