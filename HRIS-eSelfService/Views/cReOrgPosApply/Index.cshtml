<script src="~/Angular/ng-cReOrgPosApply.js"></script>
<section ng-app="ng_selfService_App" ng-controller="cReOrgPosApply_ctrl">
    <!-- Add Modal -->
    <div class="modal fade" id="main_modal" role="dialog" aria-labelledby="modalLabelSmall" aria-hidden="true">
        <div class="modal-dialog modal-lg">
            <div class="modal-content animated flipInY">
                <div class="modal-header modal-header-add-edit text-white">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <h4 class="modal-title" id="modalLabelSmall">{{ModalTitle}}</h4>
                </div>
                <div class="modal-body with-background" style="padding-bottom:5px;padding-top:5px;">
                    <div class="row" style="padding-top:5px;">
                        <div class="col-sm-2">
                            <label class="pull-left font-bold" style="padding-top:7px;font-size:12px;"><b>Budget Year:</b></label>
                        </div>
                        <div class="col-sm-4">
                            <input type="text" id="txtb_budget_code" ng-model="ddl_budget_year" class="form-control text-center" ng-disabled="true">
                        </div>
                        <div class="col-sm-6">
                            <div style="width:37%; float:left;">
                                <label class="pull-left font-bold" style="padding-top:7px;"><b>Employment Type:</b></label>
                            </div>
                            <div style="width:63%; float:left;">
                                <select ng-disabled="true" class="form-control field-select-sm" id="ddl_employment_type" ng-model="ddl_employment_type">
                                    <option value="" selected>--Select Here--</option>
                                    <option ng-repeat="lst in employment_types_list" value="{{lst.employment_type}}">{{lst.employmenttype_description}}</option>
                                </select>
                                <label id="lbl_ddl_employment_type_req" class="lbl_required"></label>
                            </div>
                        </div>
                    </div>
                    <div class="row" style="padding-top:5px;">
                        <div class="col-sm-2">
                            <label class="pull-left font-bold" style="padding-top:7px;font-size:12px;"><b>Department:</b></label>
                        </div>
                        <div class="col-sm-10">
                            <select class="form-control field-select-sm" id="ddl_department" ng-disabled="dis_as_type" ng-model="ddl_department" ng-change="get_plantilla_items()">
                                <option value="" selected>--Select Here--</option>
                                <option ng-repeat="lst in departments_list" value="{{lst.department_code}}">{{lst.department_name1}}</option>
                            </select>
                            <label id="lbl_ddl_budget_year_req" class="lbl_required"></label>
                        </div>
                    </div>
                    <div class="row" ng-show="fund_show" style="padding-top:5px;">
                        <div class="col-sm-2">
                            <label class="pull-left font-bold" style="padding-top:7px;font-size:12px;"><b>Fund Charges:</b></label>
                        </div>
                        <div class="col-sm-10">
                            <select class="form-control field-select-sm" id="ddl_fund_charges" ng-model="ddl_fund_charges" ng-change="get_plantilla_items()">
                                <option value="" selected>--Select Here--</option>
                                <option ng-repeat="lst in fundchargest_list" value="{{lst.fund_code}}">{{lst.fund_description}}</option>
                            </select>

                        </div>
                    </div>
                    <div class="row" style="padding-top:5px;">
                        <div class="col-sm-2">
                            <label class="pull-left font-bold" style="padding-top:7px;font-size:12px;"><b>Search:</b></label>
                        </div>
                        <div class="col-sm-10">
                            <input type="search" class="form-control" ng-model="search_box" ng-keyup="search_in_list(search_box,'datalist_grid')" placeholder="Search ....">
                        </div>
                    </div>

                    <hr style="margin-top:5px;margin-bottom:2px;border:0.5px solid cadetblue;padding:0px;" />
                    <div class="row" style="padding-top:5px;">
                        <div class="col-sm-12 " style="min-height:380px;">
                            <div class="table-wrapper-scroll-y table-responsive">
                                <table id="datalist_grid" ng-model="datalist_grid" class="table_div table table-striped table-bordered table-hover table-responsive" style="width:100% !important;">
                                    <thead>
                                        <tr>
                                            <th width="15%">ITEM No.</th>
                                            <th width="57%">POSITION TITLE</th>
                                            <th width="18%">MONTHLY RATE</th>
                                            <th width="10%">ACTION</th>
                                        </tr>
                                    </thead>
                                    <tbody></tbody>
                                </table>
                            </div>
                        </div>

                    </div>
                </div>
                <div class="modal-footer" style="padding: 10px 30px 10px 30px !important">

                </div>
            </div>
        </div>
    </div>
    <!--End of Add Modal-->
    <!--MODAL FOR QS-->
