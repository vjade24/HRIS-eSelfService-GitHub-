                                                    <label style="font-weight:bold;"><i class="fa fa-info-circle text-info"></i> {{datalistgrid[curr_selected_pos]["qs_eligibility"]}}</label>
                                                </div>
                                            </div>
                                            <hr style="border-color: #5b2703;" />
                                            <div class="row">
                                                <div class="col-lg-12">
                                                    <label style="font-weight:bold;">Your Eligibility</label>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-lg-12">
