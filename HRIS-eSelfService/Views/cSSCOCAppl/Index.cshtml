
@{
    ViewBag.Title = "Index";
}
<script src="~/Angular/ng-cSSCOCAppl.js"></script>
<style type="text/css">

    .popover.clockpicker-popover {
        z-index: 2060;
    }
</style>
<section ng-app="ng_eSelfService_App" ng-controller="cSSCOCAppl_ctrl">

    <div class="modal fade" id="modal_loading">
        <div class="modal-dialog">
            <div class="modal-dialog text-center">
                <div class="spiner-example">
                    <div class="sk-spinner sk-spinner-cube-grid" style="width:250px;height:250px;">
                        <div class="sk-cube"></div>
                        <div class="sk-cube"></div>
                        <div class="sk-cube"></div>
                        <div class="sk-cube"></div>
                        <div class="sk-cube"></div>
                        <div class="sk-cube"></div>
                        <div class="sk-cube"></div>
                        <div class="sk-cube"></div>
                        <div class="sk-cube"></div>
                        <div class="row">
                            <div class="spiner-example text-white" style="margin-left:20px;">
                                <span style="float:left;font-size:16px;" id="loading_msg">LOADING</span>
                                <div style="float:left;width:150px;" class="sk-spinner sk-spinner-three-bounce">
                                    <div class="sk-bounce1" style="background-color:white;"></div>
                                    <div class="sk-bounce2" style="background-color:white;"></div>
                                    <div class="sk-bounce3" style="background-color:white;"></div>
                                    <div class="sk-bounce3" style="background-color:white;"></div>
                                    <div class="sk-bounce3" style="background-color:white;"></div>
                                    <div class="sk-bounce3" style="background-color:white;"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    @*---ADD MODAL-----*@
    <div class="modal fade" id="main_modal" tabindex="-1" role="dialog" aria-labelledby="modalLabelSmall" aria-hidden="true">
        <div class="modal-dialog modal-md">
            <div class="modal-content modal-content-add-edit">
                <div class="modal-header modal-header-add-edit text-white">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                    <h4 class="modal-title" id="modalLabelSmall">{{ModalTitle}}</h4>
                </div>
                <div class="modal-body with-background" style="padding-bottom:5px;padding-top:5px;">

                    <div class="row">
                        <div class="col-lg-10">
                            <span class="pull-left font-bold" style="padding-top: 5px;"><b>Employee's Name:</b></span>
                            <input type="text" id="txtb_employee_name" ng-model="txtb_employee_name" style="font-weight:bold" class="form-control form-control-sm" disabled ng-show="!empl_name" />
                        </div>
                        <div class="col-lg-2">
                            <span class="pull-left font-bold" style="padding-top: 5px;"><b>ID No:</b></span>
                            <input type="text" id="txtb_empl_id" ng-model="txtb_empl_id" style="font-weight:bold" class="form-control form-control-sm text-center" disabled />
                        </div>
                        <div class="col-lg-12">
                            <hr style="margin-top:10px !important;margin-bottom:10px !important" />
                        </div>
                    </div>

                    <div class="row" style="margin-top:5px;">
                        <div class="col-lg-12 text-center">
                            <span class="font-bold m-t-xs"><b>Daily Time Record Date:</b></span>
                        </div>
                        <div class="col-lg-12">
                            <input type="text" id="txtb_dtr_date" ng-model="txtb_dtr_date" style="font-weight:bold;height:50px;font-size:x-large;background-color: white;" class="form-control form-control-sm text-center" disabled />
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-sm-6" style="padding-bottom:0px;margin-bottom:0px;">
                            <ul class="sortable-list connectList agile-list" id="todo">
                                <li class="success-element" id="task2">
                                    <h3>AM ENTRIES</h3>
                                    <div class="agile-detail">
                                        <div id="clockpicker_icn">
                                            <div class="input-group clockpicker" style="margin-bottom:5px;" data-placement="top" data-align="top" data-autoclose="true">
                                                <span class="input-group-addon" >
                                                    <label style="width:35px;">IN</label>
                                                </span>
                                                <input type="text" id="txtb_time_in_am" ng-model="txtb_time_in_am" style="font-weight:bold;" class="form-control form-control-sm text-center" maxlength="8"  />
                                            </div>
                                        </div>
                                        <div id="clockpicker_icn2">
                                            <div class="input-group clockpicker" style="margin-bottom:5px;" data-placement="top" data-align="top" data-autoclose="true">
                                                <span class="input-group-addon">
                                                    <label style="width:35px;">OUT</label>
                                                </span>
                                                <input type="text" id="txtb_time_out_am" ng-model="txtb_time_out_am" style="font-weight:bold;" class="form-control form-control-sm text-center" maxlength="8"   />
                                            </div>
                                        </div>
                                    </div>
                                </li>
                            </ul>
                        </div>
                        <div class="col-sm-6" style="padding-bottom:0px;margin-bottom:0px;">
                            <ul class="sortable-list connectList agile-list" id="todo">
                                <li class="danger-element" id="task2">
                                    <h3>PM ENTRIES</h3>
                                    <div class="agile-detail">
                                        <div id="clockpicker_icn3">
                                            <div class="input-group clockpicker" style="margin-bottom:5px;" data-placement="top" data-align="top" data-autoclose="true">
                                                <span class="input-group-addon">
                                                    <label style="width:35px;">IN</label>
                                                </span>
                                                <input type="text" id="txtb_time_in_pm" ng-model="txtb_time_in_pm" style="font-weight:bold;" class="form-control form-control-sm text-center" maxlength="8"   />
                                            </div>
                                        </div>
                                        <div id="clockpicker_icn4">
                                            <div class="input-group clockpicker" style="margin-bottom:5px;" data-placement="top" data-align="top" data-autoclose="true">
                                                <span class="input-group-addon">
                                                    <label style="width:35px;">OUT</label>
                                                </span>
                                                <input type="text" id="txtb_time_out_pm" ng-model="txtb_time_out_pm" style="font-weight:bold;" class="form-control form-control-sm text-center" maxlength="8"   />
                                            </div>
                                        </div>
                                    </div>
                                </li>
                            </ul>
                        </div>
                    </div>




                    <div class="row" style="margin-top:5px;display:none!important">
                        <div class="col-lg-4">
                            <span class="pull-left font-bold"><b>DTR Status:</b></span>
                            <input type="text" id="txtb_dtr_status" ng-model="txtb_dtr_status" style="font-weight:bold;" class="form-control form-control-sm" />

                        </div>
                        <div class="col-lg-4">
                            <span class="pull-left font-bold"><b>Processed User:</b></span>
                            <input type="text" id="txtb_processed_by_user" ng-model="txtb_processed_by_user" style="font-weight:bold;" class="form-control form-control-sm" />

                        </div>
                        <div class="col-lg-4">
                            <span class="pull-left font-bold"><b>Processed Date Time:</b></span>
                            <input type="text" id="txtb_processed_dttm" ng-model="txtb_processed_dttm" style="font-weight:bold;" class="form-control form-control-sm" />
                        </div>
                    </div>

                </div>
                <div class="modal-footer" style="padding: 10px 30px 10px 30px !important">
                    <button type="button" class="btn btn-danger" data-dismiss="modal"><i class="fa fa-close"></i> Close</button>
                    <button id="btn_save" ng-click="btn_save_click()" type="button" class="btn btn-success"><i class="fa fa-save" id="i_save"></i> Save</button>
                </div>
            </div>
        </div>
    </div>

    @*---END ADD MODAL-----*@

    <div class="wrapper wrapper-content animated fadeInRight">
        <div class="row">
            <div class="col-lg-12">
                <div class="ibox float-e-margins">
                    <div class="ibox-heading">
                        <div class="ibox-title ddo-bg-1" style="background-color:#507cd1;border-radius:5px 5px 0px 0px;">
                            <div class="row">
                                <div class="col-lg-3">
                                    <h2 class="text-white">DTR From Bio</h2>
                                </div>

                                <div class="col-lg-9" >
                                    <div class="form-group row">
                                        <div class="col-lg-2">
                                            <select class="form-control" id="ddl_year" ng-model="ddl_year" ng-change="FilterPageGrid()">
                                                <option ng-repeat="l in year" value="{{l.year}}">{{l.year}}</option>
                                            </select>
                                        </div>
                                        <div class="col-lg-2">
                                            <select class="form-control" id="ddl_month" ng-model="ddl_month" ng-change="FilterPageGrid()">
                                                <option value="01">January</option>
                                                <option value="02">February</option>
                                                <option value="03">March</option>
                                                <option value="04">April</option>
                                                <option value="05">May</option>
                                                <option value="06">June</option>
                                                <option value="07">July</option>
                                                <option value="08">August</option>
                                                <option value="09">September</option>
                                                <option value="10">October</option>
                                                <option value="11">November</option>
                                                <option value="12">December</option>
                                            </select>
                                        </div>
                                        <div class="col-lg-8">
                                            <select class="select2_demo_3 form-control" id="ddl_name" ng-model="ddl_name" ng-change="FilterPageGrid()" style="width:100%;">
                                                <option value="">-- Employee Name --</option>
                                                <option ng-repeat="lst in empl_names" value="{{lst.empl_id}}">{{lst.employee_name}} - {{lst.empl_id}}</option>
                                            </select>
                                        </div>
                                        
                                    </div>
                                </div>

                            </div>
                        </div>
                    </div>
                    <div class="ibox-content">
                        
                        <div class="row" >
                            <div class="col-lg-12 table-resposive">
                                <table id="datalist_grid" class="table table-striped table-bordered table-hover table-responsive ddo-grid-bg" style="width:100% !important;">
                                    <thead>
                                        <tr>
                                            <th width="10%">DATE</th>
                                            <th width="20%">TIME IN AM</th>
                                            <th width="20%">TIME OUT AM</th>
                                            <th width="20%">TIME IN PM</th>
                                            <th width="20%">TIME OUT PM</th>
                                            <th width="10%">ACTION</th>
                                        </tr>
                                    </thead>
                                    <tbody></tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

