                                    <option ng-repeat="lst in reason_list" value="{{lst.reason_code}}">{{lst.reason_descr}}</option>
                                </select>

                                <input type="text" id="txtb_reason_descr" ng-show="hr_enable_only"  maxlength="100" ng-model="txtb_reason_descr" class="form-control text-center" ng-disabled="true">
