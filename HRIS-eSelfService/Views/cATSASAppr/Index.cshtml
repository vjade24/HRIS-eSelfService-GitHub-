                                <input type="text" ng-disabled="{{isEdit}}" class="form-control text-success ng-pristine" maxlength="7" placeholder="TIME IN" id="txtb_pm_in" ng-model="txtb_pm_in">
                            </div>
                            <div class="col-lg-2">
                                <input type="text" ng-disabled="{{isEdit}}" class="form-control text-danger ng-pristine" maxlength="7" placeholder="TIME OUT" id="txtb_pm_out" ng-model="txtb_pm_out">
                            </div>

                        </div>

