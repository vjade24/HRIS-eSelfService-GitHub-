
<script src="~/Angular/ng-cSSSearchEmpl.js"></script>
<style type="text/css">
    @@media only screen and (max-width:1030px) {
        .div_select_show {
            width: 50% !important;
        }

        .div_select_year {
            width: 50% !important;
            float: right !important;
            padding-right: 0px !important;
        }
    }

    @@media only screen and (max-width:545px) {
        .div_select_show {
            width: 100% !important;
        }

        .div_select_year {
            width: 100% !important;
            float: right !important;
            padding-right: 0px !important;
            padding-top: 5px !important;
        }


        .div_span_show {
            margin-right: 55px !important;
        }
    }

    span.details-control {
        background: url('../../ResourcesImages/show.jpg') no-repeat center center;
        background-size: 20px;
        width: 20px;
        height: 20px;
        cursor: pointer;
    }

    tr.shown span.details-control {
        background: url('../../ResourcesImages/unshow.jpg') no-repeat center center;
        background-size: 20px;
        width: 20px;
        height: 20px;
        cursor: pointer;
    }

    @@media only screen and (max-width:625px) {

        thead tr th {
            font-size: 11px !important;
        }

        tbody tr td {
            font-size: 12px !important;
        }
    }

    @@media only screen and (max-width:360px) {

        .h2_class {
            font-size: 20px !important;
        }
    }

    @@media only screen and (max-width:767px) {

        .ibox-content {
            clear: both;
            padding-right: 0px;
        }
    }

    @@media only screen and (max-width:400px) {

        thead tr th {
            font-size: 9px !important;
        }

        tbody tr td {
            font-size: 11px !important;
        }
    }

    @@media only screen and (max-width:361px) {

        thead tr th {
            font-size: 8px !important;
        }

        tbody tr td {
            font-size: 10px !important;
        }
    }
</style>
<section ng-app="ng_eSelfService_App" ng-controller="cSSSearchEmpl_ctrl">

    <div class="modal fade" id="modal_loading">
        <div class="modal-dialog">
            <div class="modal-dialog text-center">
                <div class="spiner-example">
                    <div class="sk-spinner sk-spinner-cube-grid" style="width:250px;height:250px;">
                        <div class="sk-cube"></div>
                        <div class="sk-cube"></div>
                        <div class="sk-cube"></div>
                        <div class="sk-cube"></div>
                        <div class="sk-cube"></div>
                        <div class="sk-cube"></div>
                        <div class="sk-cube"></div>
                        <div class="sk-cube"></div>
                        <div class="sk-cube"></div>
                        <div class="row">
                            <div class="spiner-example text-white" style="margin-left:20px;">
                                <span style="float:left;font-size:16px;" id="loading_msg">LOADING</span>
                                <div style="float:left;width:150px;" class="sk-spinner sk-spinner-three-bounce">
                                    <div class="sk-bounce1" style="background-color:white;"></div>
                                    <div class="sk-bounce2" style="background-color:white;"></div>
                                    <div class="sk-bounce3" style="background-color:white;"></div>
                                    <div class="sk-bounce3" style="background-color:white;"></div>
                                    <div class="sk-bounce3" style="background-color:white;"></div>
                                    <div class="sk-bounce3" style="background-color:white;"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="wrapper wrapper-content animated fadeInRight">
        <div class="row">
            <div class="col-lg-12">
                <div class="ibox float-e-margins">
                    <div class="ibox-heading">
                        <div class="ibox-title" style="background-color:#507cd1;border-radius:5px 5px 0px 0px;">
                            <div class="row">
                                <div class="col-lg-4">
                                    <h2 class="text-white h2_class">@Session["menu_name"].ToString()</h2>
                                </div>
                                @*<div class="col-lg-8" style="margin-bottom:3px;">
                                        <input type="search" class="form-control" ng-model="search_box_dochistory" ng-keyup="search_in_list(search_box_dochistory,'datalist_dochistory')" placeholder="Search ....">
                                    </div>*@
                            </div>
                        </div>
                    </div>
                    <div class="ibox-content">
                        <div class="row">
                            <div class="col-sm-3 col-md-3 col-lg-3 div_select_show">
                                <span class="pull-left font-bold div_span_show" style="margin-top:9px;margin-right:10px;"><b>Show</b></span>
                                <select class="form-control field-select-sm setTableRow" style="width:30%;float:left;" ng-model="rowLen" tableref="find_empl_grid">
                                    <option value="10" selected>10</option>
                                    <option value="20">20</option>
                                    <option value="50">50</option>
                                    <option value="100">100</option>
                                </select>
                                <span class="font-bold" style="float:left;margin-top:9px;margin-left:10px;"><b>entries</b></span>
                            </div>

                            <div class="col-sm-4 col-md-4 col-lg-4 col-xs-4" style="margin-top:-10px;">
                                <span class="pull-left font-bold" style="margin-top:9px;margin-right:10px;"><b>Last Name : </b></span>
                                <input type="text" class="form-control" id="lastname" ng-model="lastname" placeholder="Search last name">
                            </div>
                            <div class="col-sm-4 col-md-4 col-lg-4 col-xs-4" style="margin-top:-10px;">
                                <span class="pull-left font-bold" style="margin-top:9px;margin-right:10px;"><b>First Name : </b></span>
                                <input type="text" class="form-control" id="firstname" ng-model="firstname" placeholder="Search first name">
                            </div>
                            <div class="col-sm-1 col-md-1 col-lg-1 col-xs-1" style="margin-top:15px;">
                                <button class="btn btn-success" id="findemployee" ng-model="findemployee" ng-click="findEmployee()">Search<i class="fa fa-search"></i></button>
                            </div>
                        </div>
                        <div class="row table-responsive" style="margin-top:10px !important">
                            <div class="col-12 ">
                                <table id="find_empl_grid" class="table table-striped table-bordered table-hover table-responsive" style="width:100% !important;">
                                    <thead>
                                        <tr>
                                            <th width="10%">ID</th>
                                            <th width="30%">EMPLOYEE NAME</th>
                                            <th width="20%">DEPARTMENT</th>
                                            <th width="20%">POSITION</th>
                                            <th width="10%">DAILY RATE</th>
                                            <th width="10%">ACTION</th>
                                        </tr>
                                    </thead>
                                    <tbody></tbody>
                                </table>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
   


    <div class="modal fade" id="details" role="dialog" aria-labelledby="modalLabelSmall" aria-hidden="true">
        <div class="modal-dialog modal-md">
            <div class="modal-content animated flipInY modal-content-add-edit">
                <div class="modal-header modal-header-add-edit text-white">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <h4 class="modal-title" id="modalLabelSmall">{{InnerModalTitle}}</h4>
                </div>
                <div class="modal-body with-background" style="padding-bottom:5px;">
                    <div class="row">
                        <div class="row mt10">
                            <div class="col-lg-3">
                                <b><label style="margin-top:9px;">Employee ID:</label></b>
                            </div>
                            <div class="col-lg-3">
                                <b><input class="form-control text-left" id="empl_id" ng-model="empl_id" disabled /></b>
                            </div>
                            <div class="col-lg-3">
                                <b><label style="margin-top:9px;">Employment Type:</label></b>
                            </div>
                            <div class="col-lg-3">
                                <b><input class="form-control text-left" id="employment_type_descr" ng-model="employment_type_descr" disabled /></b>
                            </div>
                        </div>
                        <hr style="margin-top:0px;margin-bottom:2px;border:0.5px solid cadetblue;padding:0px;" />
                        <div class="row mt10">
                            <div class="col-sm-12">
                                <div class="row">
                                    <div class="col-lg-3">
                                        <span class="pull-left" style="margin-top:9px;"><b>Employee Name:</b></span>
                                    </div>
                                    <div class="col-lg-9">
                                        <b><input class="form-control text-left" id="txtb_transmittal_nbr" ng-model="employee_name" disabled /></b>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row mt10">
                            <div class="col-sm-12">
                                <div class="row">
                                    <div class="col-lg-3">
                                        <span class="pull-left" style="margin-top:9px;"><b>Department:</b></span>
                                    </div>
                                    <div class="col-lg-9">
                                        <b><input class="form-control text-left" id="department_name1" ng-model="department_name1" disabled /></b>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row mt10">
                            <div class="col-sm-12">
                                <div class="row">
                                    <div class="col-lg-3">
                                        <span class="pull-left" style="margin-top:9px;"><b>Position:</b></span>
                                    </div>
                                    <div class="col-lg-9">
                                        <b><input class="form-control text-left" id="position_long_title" ng-model="position_long_title" disabled /></b>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <hr style="margin-top:5px;margin-bottom:2px;border:0.5px solid cadetblue;padding:0px;" />
                        <div class="row mt10">
                            <div class="col-lg-3">
                                <b><label style="margin-top:9px;">Rate Basis:</label></b>
                            </div>
                            <div class="col-lg-3">
                                <b><input class="form-control text-left" id="rate_basis_descr" ng-model="rate_basis_descr" disabled /></b>
                            </div>
                            <div class="col-lg-3">
                                <b><label style="margin-top:9px;">Salary Rate:</label></b>
                            </div>
                            <div class="col-lg-3">
                                <b><input class="form-control text-left" id="salary_amount" ng-model="salary_amount" disabled /></b>
                            </div>
                        </div>

                        <hr style="margin-top:5px;margin-bottom:2px;border:0.5px solid cadetblue;padding:0px;" />

                    </div>
                        <div class="modal-footer" style="padding: 10px 30px 10px 30px !important">
                            <button type="button" class="btn btn-danger" data-toggle="tooltip" data-placement="left" title="Close" data-dismiss="modal"><i class="fa fa-close"></i> Close</button>

                        </div>
                    </div>
        </div>
    </div>
</section>

